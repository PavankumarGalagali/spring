package com.tyss.mycollection.set;

public class Student {
	int id;
	String name;
	int standard;
	double marks;
	
	public Student() {
		super();
	}

	public Student(int id, String name, int standard, double marks) {
		super();
		this.id = id;
		this.name = name;
		this.standard = standard;
		this.marks = marks;
	}
	
	
}
