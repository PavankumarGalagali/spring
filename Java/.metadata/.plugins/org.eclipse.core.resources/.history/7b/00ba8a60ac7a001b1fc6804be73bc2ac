package com.tyss.collectionimpl.mycollection;

public class MyArray {
	
	int[] arr;
	int position = 0;
	final static  int DEFAULT_SIZE = 10;
	
	public MyArray() {
		arr =  new int[DEFAULT_SIZE];
	}
	
	public  MyArray(int size) {
		arr = new int[size];
	}
	
	public boolean add(int element) {
		arr[position++] = element;
		return true;
	}

}
