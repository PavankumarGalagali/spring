package com.tyss.thread.multithread;

public class Threads {

	public static void main(String[] args) {
		String name1 = "ABC";
		String name2 = "ABC";
		
		Thread tt1 = new Thread() {
			public void run() {
				synchronized (name1) {
				System.out.println("thread 1 : name1");	
				}
				
			};
		}
	}
}
